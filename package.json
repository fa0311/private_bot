{
  "name": "private_bot",
  "version": "1.0.0",
  "description": "Next generation bot using @line/bot-sdk and discord.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:snap": "tsx watch src/snap.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:snap": "node dist/snap.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --fix src",
    "format": "biome format --write src"
  },
  "dependencies": {
    "@line/bot-sdk": "^10.1.2",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.13",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "twitter-openapi-typescript": "^0.0.55",
    "twitter-openapi-typescript-generated": "^0.0.38",
    "twitter-snap": "^0.0.77",
    "webdav": "^5.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.4",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "bot",
    "line",
    "discord",
    "typescript",
    "esm"
  ],
  "author": "yuki",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0"
}
